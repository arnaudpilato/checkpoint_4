{% extends 'admin.html.twig' %}

{% block title %}Club{% endblock %}

{% block body %}
    <h1>Club</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ club.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ club.name }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ club.longitude }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ club.latitude }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ club.description }}</td>
            </tr>
            <tr>
                <th>Picture</th>
                <td>{{ club.picture }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ club.updatedAt ? club.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_club_index') }}">back to list</a>

    <a href="{{ path('admin_club_edit', {'id': club.id}) }}">edit</a>

    {{ include('admin/club/_delete_form.html.twig') }}
{% endblock %}
