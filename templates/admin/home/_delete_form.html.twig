<form method="post" action="{{ path('admin_home_delete', {'id': home.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ home.id) }}">
    <button class="btn btn-secondary mt-2">Delete</button>
</form>
